#include <iostream>
using namespace std;
int main()
{
	int n;
	cout << "Enter number of rows to print \n";
	cin >> n;
	int i = 1;
	while (i <= n)
	{
		int j = 1;
		while (j <= n)
		{
			cout << j;
			// cout << n-j+1; //this will print the reverse of above code
			j++;
		}
		
		cout << endl;
		i++;
	}

	return 0;
}